                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT c.DNI, c.Nombre, c.Apellido, esp.Descripcion AS Especialidad, e.Nombre AS Estudio, a.FechaDesde, a.FechaHasta, t.FechaHoraTurno AS FechaTurno
FROM   Turno AS t 
INNER JOIN Cliente AS c ON t.Cliente = c.Id 
INNER JOIN Agenda AS a ON a.Id = t.Agenda
INNER JOIN Entidad AS s ON a.Entidad = s.Id 
INNER JOIN Estudio AS e ON a.Estudio = e.Id 
INNER JOIN Especialidad AS esp ON esp.Id = e.Especialidad 
WHERE (a.FechaDesde &gt; @fechaDesde) AND (a.FechaHasta &lt; @fechaHasta) AND (e.Id = @estudio)</CommandText>
