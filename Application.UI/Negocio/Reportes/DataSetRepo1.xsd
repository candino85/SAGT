                    <CommandText>SELECT esp.Descripcion AS ESPECIALIDAD, est.Nombre AS ESTUDIO, cli.DNI AS DNI_CLIENTE, cli.Apellido AS APE_CLIENTE, cli.Nombre AS NOM_CLIENTE, tur.FechaHoraTurno AS FECHAHORA_TURNO, mue.Operacion AS OPERACION_MUESTRA, mue.Usuario AS USUARIO, mue.FechaHora AS FECHAHORA_MUESTRA
FROM   Muestra AS mue INNER JOIN
            Turno AS tur ON tur.Id = mue.Turno INNER JOIN
            Cliente AS cli ON cli.Id = tur.Cliente INNER JOIN
            Agenda AS age ON age.Id = tur.Agenda INNER JOIN
